<idea-plugin>
  <id>com.bakerj.mvp.generator.id</id>
  <name>MVPGenerator</name>
  <version>1.3</version>
  <vendor email="305317218@qq.com" url="http://www.bakerjq.com">BakerJ</vendor>

  <description><![CDATA[
      MVPGenerator.<br>
      <em></em>
    ]]></description>

  <change-notes><![CDATA[
      Fix Windows<br>
      <em></em>
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
